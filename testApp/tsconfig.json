{
  "extends": "@sveltejs/kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "module": "esnext",
    "jsx": "preserve",
    "moduleResolution": "node",
    "lib": ["dom", "es2017"],
    "strictNullChecks": true
  }
}
